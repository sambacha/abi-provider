  "@stacks/prettier-config": "0.0.9",
  "@testing-library/react": "12.1.2",
  "@types/merge-deep": "3.0.0",
  "@types/react": "17.0.28",
  "@types/react-dom": "17.0.9",
  "@typescript-eslint/eslint-plugin": "5.0.0",
  "@typescript-eslint/parser": "5.0.0",
  "babel-jest": "27.2.5",
  "concurrently": "6.3.0",

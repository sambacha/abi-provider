{
  "name": "abi-provider",
  "version": "0.1.0",
  "description": "web3 provider for ABIs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn run build:base",
    "build:base": "rollup -c",
    "build:nextjs": "rollup -c --config-sdk",
    "pretest": "tsc --noEmit",
    "test": "NODE_ENV=test jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "fmt": "npx prettier --config .prettierrc.js --write src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sambacha/abi-provider.git"
  },
  "keywords": [],
  "author": "sam bacha",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sambacha/abi-provider/issues"
  },
  "homepage": "https://github.com/sambacha/abi-provider#readme",
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.17.12",
    "@babel/plugin-transform-react-jsx": "7.14.9",
    "@babel/plugin-transform-runtime": "7.18.2",
    "@babel/plugin-transform-typescript": "7.18.4",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "21.0.0",
    "@rollup/plugin-node-resolve": "13.0.5",
    "@rollup/plugin-typescript": "8.3.2",
    "@testing-library/react": "12.1.2",
    "@types/chai": "^4.3.1",
    "@types/chrome": "^0.0.188",
    "@types/jest": "^27",
    "@types/merge-deep": "3.0.0",
    "@types/mocha": "^9.1.1",
    "@types/react": "17.0.28",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "babel-jest": "27.2.5",
    "chai": "^4.3.6",
    "concurrently": "6.3.0",
    "downlevel-dts": "0.7.0",
    "esbuild": "^0.14.48",
    "fast-async": "6.3.8",
    "jest": "^27",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-size-snapshot": "0.12.0",
    "shx": "0.3.3",
    "ts-jest": "27.0.5",
    "tslib": "2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.6.4",
    "@ethersproject/keccak256": "^5.6.1",
    "@ethersproject/strings": "^5.6.1",
    "fast-deep-equal": "3.1.3",
    "keccak256": "^1.0.6"
  }
}
